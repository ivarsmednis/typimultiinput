{
    "name": "ivarsmednis/typimultiinput",
    "description": "Extension for TypiCMS. For making multi value fields. Forked from visermort/typimultiinput",
    "license": "MIT",
    "keywords": ["cms", "multilingual", "laravel", "typi", "extension", "multiinput"],
    "authors": [
        {
            "name": "Ivars",
            "email": "ivars.mednis@g-i.lv"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Ivarsmednis\\TypiMultiInput\\": "src/MultiInput"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ivarsmednis\\TypiMultiInput\\MultiInputProvider"
            ],
            "aliases": {
                "MultiInput":"Ivarsmednis\\TypiMultiInput\\MultiInput"
            }
        }
    }
}
